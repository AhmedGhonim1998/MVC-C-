@model List<Departement>
@{
    ViewData["Title"] = "Depatement Index";
}

<h1>Index</h1>
<a class="btn"></a>

<table class="table table-hover table-bordered">
    <tr>
        <th>
            No.
        </th>
        <th>
            Name
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.Id
            </td>

            <td>
                @item.Name
            </td>
        </tr>
    }

    <a asp-action="Add"
       asp-controller="Departement"
       class="btn btn-secondary">
        go to Add
    </a>
</table>
